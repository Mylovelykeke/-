<import src="/components/chat/index.vue.wxml" />
<import src="/components/commonent_item/index.vue.wxml" /><template name="08021537">
  <view class="_div data-v-c0ff5b1a main">
    <view class="_div data-v-c0ff5b1a nav">
      <view class="_div data-v-c0ff5b1a back">
        <view class="_i data-v-c0ff5b1a iconfont icon-fanhui" bindtap="handleProxy" data-eventid="{{'0'}}" data-comkey="{{$k}}"></view>
      </view>
      <swiper wx:if="{{files&&files.length>0}}" class="_swiper data-v-c0ff5b1a swiper" indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
        <swiper-item wx:key="index" key="{{index}}" wx:for="{{files}}" wx:for-index="index" wx:for-item="item" class="_swiper-item data-v-c0ff5b1a">
          <image src="{{item.url}}" data-src="{{item.url}}" class="_image data-v-c0ff5b1a slide-image" mode="aspectFill" bindtap="handleProxy" data-eventid="{{'1_'+index}}" data-comkey="{{$k}}"></image>
        </swiper-item>
      </swiper>
    </view>
    <view class="_div data-v-c0ff5b1a detail_userinfo" style=" {{('padding-top:' + (files && files.length > 0 ? '' : '80px') + ';')}}">
      <image src="{{userInfo.avatar}}" alt class="_img data-v-c0ff5b1a"></image> <label class="_span data-v-c0ff5b1a user-name">{{userInfo.name}}</label> <label class="_span data-v-c0ff5b1a attest">
        <view class="_i data-v-c0ff5b1a iconfont icon-shimingrenzheng"></view>
      </label> <label class="_span data-v-c0ff5b1a flower" wx:key="count" key="{{count}}" wx:for="{{5}}" wx:for-index="index" wx:for-item="count">
        <view class="_i data-v-c0ff5b1a iconfont icon-icon-test"></view>
      </label>
    </view>
    <view class="_div data-v-c0ff5b1a content" bindtap="handleProxy" data-eventid="{{'2'}}" data-comkey="{{$k}}">
      <view class="_div data-v-c0ff5b1a detail-item">
        <view class="_div data-v-c0ff5b1a title">
          {{title}}
        </view>
      </view>
      <view class="_div data-v-c0ff5b1a location" wx:if="{{location}}">
        <view class="_div data-v-c0ff5b1a location-info">
          <view class="_i data-v-c0ff5b1a iconfont icon-zb"></view>
          {{location&&location.address}}
        </view>
        <view class="_div data-v-c0ff5b1a location-right">
          <view class="_i data-v-c0ff5b1a iconfont icon-youjiantou"></view>
        </view>
      </view>
      <view class="_div data-v-c0ff5b1a article">
        {{content}}
      </view>
      <view class="_div data-v-c0ff5b1a content-footer">
        <view class="_div data-v-c0ff5b1a"> 发表于 {{itemInfo&&itemInfo.createAt}}
        </view>
        <view class="_div data-v-c0ff5b1a content-phone">
          <view class="_div data-v-c0ff5b1a">
            <view class="_i data-v-c0ff5b1a iconfont icon-dianhua"></view> <label class="_span data-v-c0ff5b1a">联系电话</label> <label class="_span data-v-c0ff5b1a content-count">200人咨询</label>
          </view> <label class="_span data-v-c0ff5b1a">获取</label>
        </view>
      </view>
    </view>
    <view class="_div data-v-c0ff5b1a notice">
      <view class="_div data-v-c0ff5b1a notice-title"><label class="_span data-v-c0ff5b1a">租房须知</label>
        <view class="_i data-v-c0ff5b1a iconfont icon-youjiantou"></view>
      </view> <label class="_span data-v-c0ff5b1a">若房东向您索要中介费，你可举报！</label>
      <view class="_div data-v-c0ff5b1a report" bindtap="handleProxy" data-eventid="{{'3'}}" data-comkey="{{$k}}">
        <view class="_i data-v-c0ff5b1a iconfont icon-jubao"></view> <label class="_span data-v-c0ff5b1a">举报</label>
      </view>
    </view>
    <view class="_div data-v-c0ff5b1a"><template bindsendMsg="handleProxy" data-eventid="{{'4'}}" data-comkey="{{$k}}" data="{{...$root[$kk+'1'], $root}}" is="5a561960"></template></view>
    <view class="_div data-v-c0ff5b1a all-commonents">
      <view class="_div data-v-c0ff5b1a commonents-title">全部评论</view> <template bindReplyName="handleProxy" data-eventid="{{'5'}}" data-comkey="{{$k}}" data="{{...$root[$kk+'2'], $root}}" is="2a6a8c4c"></template>
    </view>
  </view>
</template>