<import src="/components/slots" /><template name="60ff0454">
  <view class="_div data-v-1b5191ff"><template name="icon"></template>
    <view class="_div data-v-1b5191ff search-bar">
      <view class="_div data-v-1b5191ff search-address" wx:if="{{region}}">
        {{region}}
        <view class="_i data-v-1b5191ff iconfont icon-xiala"></view>
      </view>
      <view class="_div data-v-1b5191ff search-content">
        <icon class="_icon data-v-1b5191ff icon-search" size="14" type="search" role="img"></icon> <input confirm-type="search" placeholder="{{tip}}" value="{{value}}" bindinput="handleProxy" bindconfirm="handleProxy" data-eventid="{{'0'}}" data-comkey="{{$k}}" class="_input data-v-1b5191ff" />
        <view class="_div data-v-1b5191ff close" hidden="{{!(value)}}" bindtap="handleProxy" data-eventid="{{'1'}}" data-comkey="{{$k}}"><template data="{{...$root[$p], ...$root[$k], $root}}" is="{{$sloticon || 'icon'}}"></template></view>
      </view>
      <view class="_div data-v-1b5191ff cancel" bindtap="handleProxy" data-eventid="{{'2'}}" data-comkey="{{$k}}"> 取消 </view>
    </view>
    <view wx:if="{{type&&!value}}" class="_div data-v-1b5191ff">
      <i-cell-group class="_i-cell-group data-v-1b5191ff">
        <i-cell title="{{v}}" wx:key="key" key="{{key}}" bindtap="handleProxy" data-eventid="{{'4_'+key}}" data-comkey="{{$k}}" wx:for="{{historyArray}}" wx:for-index="key" wx:for-item="v" class="_i-cell data-v-1b5191ff">
          <view slot="icon" class="_div data-v-1b5191ff">
            <view class="_i data-v-1b5191ff iconfont icon-lishi"></view>
          </view>
          <view slot="footer" class="_div data-v-1b5191ff">
            <view class="_i data-v-1b5191ff iconfont icon-close" bindtap="handleProxy" data-eventid="{{'3_'+key}}" data-comkey="{{$k}}"></view>
          </view>
        </i-cell>
      </i-cell-group>
      <view class="_div data-v-1b5191ff clear" bindtap="handleProxy" hidden="{{!(historyArray.length>0)}}" data-eventid="{{'5'}}" data-comkey="{{$k}}"> 清空搜索记录 </view>
    </view>
  </view>
</template>